{
  "name": "puppeteer-runner-vps",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "install:browser": "npx puppeteer browsers install chrome",
    "start": "npm run install:browser && pm2 delete alxzy-puppeteer || true && pm2 start api/run.js --name alxzy-puppeteer && pm2 save",
    "logs": "pm2 logs alxzy-puppeteer",
    "stop": "pm2 stop alxzy-puppeteer"
  },
  "dependencies": {
    "puppeteer": "latest",
    "express": "latest",
    "axios": "latest"
  }
}
